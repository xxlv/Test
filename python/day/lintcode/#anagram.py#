class Solution:
    """
    @param s: The first string
    @param b: The second string
    @return true or false
    """
    
    def anagram(self, s, t):
    
        sum_s=reduce(lambda x,y: x+y, self.get_ord_map(s))
        sum_t=reduce(lambda x,y: x+y, self.get_ord_map(t))
        return sum_s-sum_t==0

    def get_ord_map(s):

        map=[]
        for c in s:
            map.append(ord(c))
        return map    
            
        
